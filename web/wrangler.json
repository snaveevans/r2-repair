{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "r2-repair-web",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-01-09",
  "compatibility_flags": ["nodejs_compat"],
  "build": {
    "command": "npm run build"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  }
}
